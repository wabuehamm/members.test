{
  "baseUrl": "https://web:8443",
  "defaultCommandTimeout": 10000,
  "ignoreTestFiles": [
    "**/__snapshots__/*",
    "**/__image_snapshots__/*"
  ],
  "env": {
    "cypress-plugin-snapshots": {
      "autoCleanUp": true,
      "imageConfig": {
        "threshold": 20.0
      }
    }
  },
  "reporter": "mochawesome",
  "reporterOptions": {
    "overwrite": false,
    "html": false,
    "json": true
  },
  "watchForFileChanges": false
}
